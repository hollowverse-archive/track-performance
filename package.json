{
  "name": "track-performance",
  "description": "An AWS Lambda function that checks and reports the performance of hollowverse.com regularly",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:hollowverse/track-performance.git",
  "author": "Muhammad Fawwaz Orabi <mfawwaz93@gmail.com>",
  "license": "Unlicense",
  "private": true,
  "scripts": {
    "pretest": "run-p -c --aggregate-output common-check lint",
    "test": "run-p -c --aggregate-output check-ts",
    "check-ts/src": "tsc --project ./src --noEmit",
    "check-ts/root": "tsc --project ./ --noEmit",
    "check-ts": "run-p 'check-ts/*'",
    "clean": "rm -rf ./dist",
    "lint": "run-p -c --aggregate-output lint-ts lint-js check-ts",
    "lint-js": "eslint '**/*.js{,x}'",
    "lint-ts/tslint": "tslint '**/*.ts{,x}' --project .  --exclude '**/node_modules/**' --exclude '**/.serverless/**'  --exclude '**/.webpack/**'  --exclude '**/dist/**'",
    "lint-ts/eslint": "eslint '**/*.ts{,x}'",
    "lint-ts": "run-p -c --aggregate-output 'lint-ts/*'",
    "validate-filenames": "validate-filenames",
    "deploy": "babel-node deploy.js",
    "lint-staged": "lint-staged",
    "clown": "clown",
    "pre-push": "yarn test",
    "pre-commit": "yarn pretest",
    "common-check": "run-p -c --aggregate-output check-prettier validate-filenames 'clown check' check-integrity",
    "check-integrity": "yarn check --integrity",
    "check-prettier": "prettier './**/*.{t,j}s{,x}' '**/*.{css,scss,json,md}' --list-different"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.46",
    "@babel/runtime": "^7.0.0-beta.46",
    "@hollowverse/utils": "^8.0.0",
    "aws-sdk": "^2.235.1",
    "bluebird": "^3.5.1",
    "common-tags": "^1.7.2",
    "date-fns": "^1.29.0",
    "got": "^8.3.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "p-throttle": "^1.1.0",
    "pixelmatch": "^4.0.2",
    "prettier": "^1.10.2",
    "webpagetest": "^0.3.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/node": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-stage-3": "^7.0.0-beta.46",
    "@hollowverse/clown": "^3.2.0",
    "@hollowverse/config": "^41.0.0",
    "@hollowverse/validate-filenames": "^3.0.2",
    "@types/bluebird": "^3.5.20",
    "@types/common-tags": "^1.4.0",
    "@types/got": "^8.3.1",
    "@types/lodash": "^4.14.104",
    "@types/node": "^8.10.17",
    "@types/p-throttle": "^1.1.0",
    "@types/pixelmatch": "^4.0.0",
    "@types/prettier": "^1.10.0",
    "@types/tmp": "^0.0.33",
    "@types/webpack": "^3.8.8",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-typescript": "^0.12.0",
    "husky": "^1.0.0-rc.1",
    "lint-staged": "^7.0.5",
    "npm-run-all": "^4.1.3",
    "serverless": "^1.26.1",
    "serverless-offline": "^3.18.0",
    "serverless-webpack": "^5.1.0",
    "ts-loader": "^4.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.8.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-language-service": "^0.9.9",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^16.0.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0",
    "yarnhook": "^0.2.0"
  },
  "lint-staged": {
    "**/*.md": [
      "prettier --write",
      "git add"
    ],
    "**/*.{j,t}s{x,}": [
      "prettier --write",
      "git add"
    ],
    "**/*.js{x,}": [
      "eslint"
    ],
    "**/*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all -s lint-staged -p -c --aggregate-output pre-commit",
      "pre-push": "yarn pre-push",
      "post-merge": "yarnhook",
      "post-checkout": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "engines": {
    "node": ">= 8.10"
  }
}
