{
  "name": "track-performance",
  "description": "An AWS Lambda function that checks and reports the performance of hollowverse.com regularly",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:hollowverse/track-performance.git",
  "author": "Muhammad Fawwaz Orabi <mfawwaz93@gmail.com>",
  "license": "Unlicense",
  "private": true,
  "scripts": {
    "pretest": "run-p -l --aggregate-output common-check lint",
    "test": "run-p -l --aggregate-output check-ts",
    "check-ts/src": "tsc --project ./src --noEmit",
    "check-ts/root": "tsc --project ./ --noEmit",
    "check-ts": "run-p 'check-ts/*'",
    "clean": "rm -rf ./dist",
    "lint": "run-p -l --aggregate-output lint-ts lint-js check-ts",
    "lint-js": "eslint '**/*.js{,x}'",
    "lint-ts": "tslint './*.ts' 'src/**/*.ts{,x}' --project .",
    "validate-filenames": "validate-filenames --config validateFilenames.js",
    "deploy": "babel-node deploy.js",
    "lint-staged": "lint-staged",
    "clown": "clown",
    "pre-push": "yarn test",
    "pre-commit": "yarn pretest",
    "tslint": "yarn lint-ts",
    "common-check": "run-p -l -c --aggregate-output validate-filenames lint-staged 'clown check'"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.44",
    "@hollowverse/utils": "^5.0.2",
    "@octokit/rest": "^14.0.9",
    "aws-sdk": "^2.212.1",
    "bluebird": "^3.5.1",
    "common-tags": "^1.7.2",
    "date-fns": "^1.29.0",
    "got": "^8.1.0",
    "lambda-git": "^0.1.2",
    "lighthouse": "^2.9.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "prettier": "^1.10.2",
    "shelljs": "^0.8.1",
    "tmp": "^0.0.33",
    "webpagetest": "^0.3.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/node": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@hollowverse/clown": "^0.0.4",
    "@hollowverse/config": "^7.1.0",
    "@hollowverse/validate-filenames": "^1.4.0",
    "@types/bluebird": "^3.5.20",
    "@types/common-tags": "^1.4.0",
    "@types/got": "^7.1.7",
    "@types/lodash": "^4.14.104",
    "@types/node": "6",
    "@types/prettier": "^1.10.0",
    "@types/shelljs": "^0.7.8",
    "@types/tmp": "^0.0.33",
    "@types/webpack": "^3.8.8",
    "babel-loader": "^8.0.0-beta.2",
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^4.18.2",
    "husky": "^0.15.0-rc.8",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.10.2",
    "serverless": "^1.26.1",
    "serverless-offline": "^3.18.0",
    "serverless-webpack": "^5.1.0",
    "ts-loader": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0",
    "yarnhook": "^0.1.1"
  },
  "lint-staged": {
    "**/*.md": [
      "prettier --write",
      "git add"
    ],
    "**/*.{j,t}s{x,}": [
      "prettier --write",
      "git add"
    ],
    "**/!(package).json": [
      "prettier --write",
      "git add"
    ],
    "**/*.js{x,}": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit",
      "pre-push": "yarn pre-push",
      "post-merge": "yarnhook",
      "post-checkout": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "engines": {
    "node": ">= 6.10"
  },
  "jest": null
}
